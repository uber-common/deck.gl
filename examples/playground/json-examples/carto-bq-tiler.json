{
  "description": "CartoBQTilerLayer declarative example",
  "initialViewState": {
    "latitude": 40.7368521,
    "longitude": -73.8536065,
    "zoom": 9,
    "pitch": 0,
    "bearing": 0
  },
  "views": [
    {
      "@@type": "MapView",
      "controller": true,
      "mapStyle": "https://basemaps.cartocdn.com/gl/positron-gl-style/style.json"    
    }
  ],
  "layers": [
    {
      "@@type": "CartoBQTilerLayer",
      "data": "cartobq.maps.nyc_taxi_points_demo_id",
      "getFillColor": {
        "@@function": "colorBins",
        "prop": "properties.avg_fare_amount",
        "breaks": [0, 10, 25, 50, 100],
        "colors": "Prism"
      },
      "pointRadiusMinPixels": 2,
      "stroked": false
    }
  ]
}