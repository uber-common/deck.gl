{
  "name": "browserify-exhibit",
  "version": "0.0.0",
  "description": "A browserify/deck.gl exhibit",
  "scripts": {
    "link": "(cd .. && rm -f node_modules && ln -s browserify/node_modules)",
    "serve": "budo ../app.js --open -- -t babelify",
    "start": "npm run link && npm run serve"
  },
  "author": "Balthazar Gronon <bgronon@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "deck.gl": "^3.1.1",
    "immutable": "^3.8.1",
    "luma.gl": "^2.10.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-map-gl": "^1.7.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babelify": "^7.3.0",
    "budo": "^9.2.2"
  },
  "babel": {
    "presets": ["es2015", "stage-2", "react"]
  }
}
