{
  "name": "project-website",
  "version": "0.0.0",
  "description": "Website for vis.gl project",
  "license": "MIT",
  "keywords": [
    "ocular"
  ],
  "main": "gatsby-config.js",
  "scripts": {
    "install-deps": "yarn && find ../examples/website -name package.json -depth 2 -execdir yarn \\;",
    "start": "yarn clean && yarn develop",
    "build": "yarn clean && gatsby build",
    "build-staging": "yarn clean && gatsby build --prefix-paths",
    "clean": "rm -rf ./.cache ./public",
    "develop": "yarn clean && gatsby develop",
    "serve": "gatsby serve",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d public"
  },
  "dependencies": {
    "d3-color": "^1.4.1",
    "d3-request": "^1.0.6",
    "d3-scale": "^3.2.1",
    "expr-eval": "^2.0.2",
    "react-jss": "^10.3.0"
  },
  "devDependencies": {
    "gatsby": "^2.20.0",
    "gatsby-plugin-jss": "^2.3.4",
    "gatsby-plugin-no-sourcemaps": "^2.1.2",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-theme-ocular": "^1.2.0-beta.8",
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "gatsby-plugin-mdx": "^1.2.16",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.14.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
