{
  "name": "@deck.gl/js",
  "description": "Scripting with deck.gl",
  "license": "MIT",
  "version": "0.0.1-alpha.1",
  "keywords": [
    "webgl",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/deck.gl.git"
  },
  "main": "es5/index.js",
  "module": "esm/index.js",
  "esnext": "es6/index.js",
  "scripts": {
    "start": "webpack-dev-server --env.dev --progress --open",
